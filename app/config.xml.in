<?xml version='1.0' encoding='utf-8'?>
<widget id="thizanne.sterne" version="%%VERSION%%" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Sterne</name>
  <description>
    A dive planning mobile application
  </description>
  <author email="thi.suzanne@gmail.com" href="https://github.com/thizanne">
    Thibault Suzanne
  </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
    <preference name="loadUrlTimeoutValue" value="700000" />
  </platform>
  <engine name="android" spec="^7.1.4" />
</widget>
